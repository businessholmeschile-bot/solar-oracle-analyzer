{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/admin",
      "has": [
        {
          "type": "host",
          "value": "www.solaroracle.cl"
        }
      ],
      "destination": "/admin.html"
    },
    {
      "source": "/demo",
      "has": [
        {
          "type": "host",
          "value": "www.solaroracle.cl"
        }
      ],
      "destination": "/dashboard_demo.html"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "app.solaroracle.cl"
        }
      ],
      "destination": "/app.html"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "app.solaroracle.cl"
        }
      ],
      "destination": "/app.html"
    }
  ]
}
