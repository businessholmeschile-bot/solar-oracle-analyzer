{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/admin",
      "has": [
        {
          "type": "host",
          "value": "www.solaroracle.cl"
        }
      ],
      "destination": "/admin.html"
    },
    {
      "source": "/demo",
      "has": [
        {
          "type": "host",
          "value": "www.solaroracle.cl"
        }
      ],
      "destination": "/dashboard_demo.html"
    },
    {
      "source": "/(index.html)?",
      "has": [
        {
          "type": "host",
          "value": "app.solaroracle.cl"
        }
      ],
      "destination": "/index.html?view=app"
    },
    {
      "source": "/admin",
      "has": [
        {
          "type": "host",
          "value": "app.solaroracle.cl"
        }
      ],
      "destination": "https://www.solaroracle.cl/admin"
    }
  ]
}
